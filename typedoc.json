{
	"entryPoints": ["src/index.ts"],
	"out": "docs-vitepress/tsdoc",
	"plugin": ["typedoc-plugin-markdown"],
	"disableSources": true,
	"hideGenerator": true,
	"readme": "none",
	"entryFileName": "index.md",
	"exclude": [
		"**/*.test.ts",
		"**/*.spec.ts",
		"src/examples/**",
		"tests/**"
	],
	"skipErrorChecking": true
}
