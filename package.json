{
  "name": "@cartago-git/quickmodel",
  "version": "1.0.0",
  "description": "Sistema de serialización/deserialización de modelos TypeScript con arquitectura SOLID",
  "author": "Cartago",
  "license": "MIT",
  "keywords": [
    "typescript",
    "models",
    "serialization",
    "deserialization",
    "solid",
    "decorators",
    "transformers"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./transformers": {
      "types": "./dist/transformers/index.d.ts",
      "import": "./dist/transformers/index.mjs",
      "require": "./dist/transformers/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.mjs",
      "require": "./dist/core/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG",
    "docs/SOLID-ARCHITECTURE.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "bun test",
    "test:all": "bun run/test-each.ts",
    "prepublishOnly": "bun run build",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "@types/bun": "^1.1.13",
    "@types/node": "^25.0.3",
    "tsup": "^8.0.1",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CartagoGit/quickmodel.git"
  },
  "bugs": {
    "url": "https://github.com/CartagoGit/quickmodel/issues"
  },
  "homepage": "https://github.com/CartagoGit/quickmodel#readme"
}
